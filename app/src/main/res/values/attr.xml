<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TakePhotoButton">
        <!--外圆颜色-->
        <attr name="outCircleColor" format="color" />
        <!--内圆颜色-->
        <attr name="innerCircleColor" format="color" />
        <!--进度条颜色-->
        <attr name="readColor" format="color" />
        <!--录制时间-->
        <attr name="maxSeconds" format="integer" />
        <!--循环圈数-->
        <attr name="maxCircleIndex" format="integer" />
        <!--是否需要展示中间的矩形-->
        <attr name="isNeedLongClick" format="boolean" />
    </declare-styleable>
    <declare-styleable name="SlideLayoutView">
        <attr name="lineWidth" format="dimension"/>
        <attr name="lineColor" format="color" />
    </declare-styleable>
    <declare-styleable name="DialView">
        <attr name="fanColor1" format="color" />
        <attr name="fanColor2" format="color" />
        <attr name="fanColor3" format="color" />
    </declare-styleable>
</resources>